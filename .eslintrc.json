{
    "parser": "babel-eslint",
    "env": {
        "es6": true,
        "browser": true,
        "node": true,
        "jest/globals": true
    },
    "rules": {
        "semi": "off",
        "linebreak-style": "off",
        "eol-last":"off",
        "arrow-parens":"off",
        "comma-dangle":"off",
        "indent":"off",
        "require-jsdoc":"off",
        "operator-linebreak":"off"
    },
    "plugins": ["jest"],
    "extends": ["eslint:recommended", "google"]
}